<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.mobile.suspect.npf" versionCode="12" version="1.2.3">
    <!-- versionCode is optional and Android only -->
    <name>NPF</name>
    <description>
        NPF Suspect Data Capture 

    </description>
    <author email="info@sinettechnologies.com" href="http://sinettechnologies.com/">
        Sinet Team
    </author>

    <content src="index.html" />

    <preference name="phonegap-version" value="3.7.0" />
    <preference name="orientation" value="default" />

    <gap:platform name="ios" />
    <gap:platform name="android" />
    <gap:platform name="winphone" />

    <!--Defaults-->
    <access origin="*" />
    <icon src="icon.png" />
    <gap:splash src="splash.png" />

    <!--=======================================================================================-->
    <!--ANDROID-->
    <preference name="android-minSdkVersion" value="10" />
    <preference name="android-targetSdkVersion" value="17" />
    <preference name="android-installLocation" value="internalOnly" />
    <preference name="SplashScreenDelay" value="3000" />

    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="screen" />


    <!--=======================================================================================-->
    <!--icons-->
    <icon src="resImages/android/icon/drawable-ldpi-icon.png" gap:platform="android" gap:qualifier="ldpi" />
    <icon src="resImages/android/icon/drawable-mdpi-icon.png" gap:platform="android" gap:qualifier="mdpi" />
    <icon src="resImages/android/icon/drawable-hdpi-icon.png" gap:platform="android" gap:qualifier="hdpi" />
    <icon src="resImages/android/icon/drawable-xhdpi-icon.png" gap:platform="android" gap:qualifier="xhdpi" />
    <icon src="resImages/android/icon/drawable-xxhdpi-icon.png" gap:platform="android" gap:qualifier="xxhdpi" />
    <icon src="resImages/android/icon/drawable-xxxhdpi-icon.png" gap:platform="android" gap:qualifier="xxxhdpi" />

    <!--splash screen-->
    <gap:splash src="resImages/android/splash/drawable-land-ldpi-screen.png" gap:platform="android" gap:qualifier="land-ldpi" />
    <gap:splash src="resImages/android/splash/drawable-land-mdpi-screen.png" gap:platform="android" gap:qualifier="land-mdpi" />
    <gap:splash src="resImages/android/splash/drawable-land-hdpi-screen.png" gap:platform="android" gap:qualifier="land-hdpi" />
    <gap:splash src="resImages/android/splash/drawable-land-xhdpi-screen.png" gap:platform="android" gap:qualifier="land-xhdpi" />
    <gap:splash src="resImages/android/splash/drawable-land-xxhdpi-screen.png" gap:platform="android" gap:qualifier="land-xxhdpi" />
    <gap:splash src="resImages/android/splash/drawable-land-xxxhdpi-screen.png" gap:platform="android" gap:qualifier="land-xxxhdpi" />
    <gap:splash src="resImages/android/splash/drawable-port-ldpi-screen.png" gap:platform="android" gap:qualifier="port-ldpi" />
    <gap:splash src="resImages/android/splash/drawable-port-mdpi-screen.png" gap:platform="android" gap:qualifier="port-mdpi" />
    <gap:splash src="resImages/android/splash/drawable-port-hdpi-screen.png" gap:platform="android" gap:qualifier="port-hdpi" />
    <gap:splash src="resImages/android/splash/drawable-port-xhdpi-screen.png" gap:platform="android" gap:qualifier="port-xhdpi" />
    <gap:splash src="resImages/android/splash/drawable-port-xxhdpi-screen.png" gap:platform="android" gap:qualifier="port-xxhdpi" />
    <gap:splash src="resImages/android/splash/drawable-port-xxxhdpi-screen.png" gap:platform="android" gap:qualifier="port-xxxhdpi" />

    <!--=======================================================================================-->
    <!--IOS-->
    <preference name="target-device" value="universal" />
    <preference name="detect-data-types" value="true" />
    <!-->>>>>>>>>>>>Icons-->

    <!-- iPhone 6 / 6+ -->
    <icon src="resImages/ios/icon-60@3x.png" gap:platform="ios" width="180" height="180" />

    <!-- iPhone / iPod Touch  -->
    <icon src="resImages/ios/icon-60.png" gap:platform="ios" width="60" height="60" />
    <icon src="resImages/ios/icon-60@2x.png" gap:platform="ios" width="120" height="120" />

    <!-- iPad -->
    <icon src="resImages/ios/icon-76.png" gap:platform="ios" width="76" height="76" />
    <icon src="resImages/ios/icon-76@2x.png" gap:platform="ios" width="152" height="152" />

    <!-- Settings Icon -->
    <icon src="resImages/ios/icon-small.png" gap:platform="ios" width="29" height="29" />
    <icon src="resImages/ios/icon-small@2x.png" gap:platform="ios" width="58" height="58" />

    <!-- Spotlight Icon -->
    <icon src="resImages/ios/icon-40.png" gap:platform="ios" width="40" height="40" />
    <icon src="resImages/ios/icon-40@2x.png" gap:platform="ios" width="80" height="80" />

    <!-- iPhone / iPod Touch -->
    <icon src="resImages/ios/icon.png" gap:platform="ios" width="57" height="57" />
    <icon src="resImages/ios/icon@2x.png" gap:platform="ios" width="114" height="114" />

    <!-- iPad -->
    <icon src="resImages/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
    <icon src="resImages/ios/icon-72@2x.png" gap:platform="ios" width="144" height="144" />

    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="resImages/ios/icon-small.png" gap:platform="ios" width="29" height="29" />
    <icon src="resImages/ios/icon-small@2x.png" gap:platform="ios" width="58" height="58" />

    <!-- iPad Spotlight and Settings Icon -->
    <icon src="resImages/ios/icon-50.png" gap:platform="ios" width="50" height="50" />
    <icon src="resImages/ios/icon-50@2x.png" gap:platform="ios" width="100" height="100" />

    <!-->>>>>>>>>>>>splash screen>>>>>>>>>>>>>>>>>>>>>>>>>>>>>-->
    <!-- iPhone and iPod touch -->
    <gap:splash src="resImages/ios/Default.png" gap:platform="ios" width="320" height="480" />
    <gap:splash src="resImages/ios/Default@2x.png" gap:platform="ios" width="640" height="960" />

    <!-- iPhone 5 / iPod Touch (5th Generation) -->
    <gap:splash src="resImages/ios/Default-568h@2x.png" gap:platform="ios" width="640" height="1136" />

    <!-- iPhone 6 -->
    <gap:splash src="resImages/ios/Default-667h@2x.png" gap:platform="ios" width="750" height="1334" />
    <gap:splash src="resImages/ios/Default-Portrait-736h@3x.png" gap:platform="ios" width="1242" height="2208" />
    <gap:splash src="resImages/ios/Default-Landscape-736h@3x.png" gap:platform="ios" width="2208" height="1242" />

    <!-- iPad -->
    <gap:splash src="resImages/ios/Default-Portrait.png" gap:platform="ios" width="768" height="1024" />
    <gap:splash src="resImages/ios/Default-Landscape.png" gap:platform="ios" width="1024" height="768" />

    <!-- Retina iPad -->
    <gap:splash src="resImages/ios/Default-Portrait@2x.png" gap:platform="ios" width="1536" height="2048" />
    <gap:splash src="resImages/ios/Default-Landscape@2x.png" gap:platform="ios" width="2048" height="1536" />
    <!--/IOS END-->
    <!--=======================================================================================-->
    <!--WINPHONE-->
    <icon src="resImages/w8/icon-50x50.png" gap:platform="winphone" />
    <icon src="resImages/w8/tileicon.png" gap:platform="winphone" gap:role="background" />
    <gap:splash src="resImages/w8/screen-620x300.jpg" gap:platform="winphone" />
    <!--/WINPHONE-->

    <!--=======================================================================================-->
    <!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
    <!--PLUGINS-->
    <gap:plugin name="org.apache.cordova.camera" version="0.3.2" />
    <gap:plugin name="org.apache.cordova.media-capture" version="0.3.3" />
    <gap:plugin name="org.apache.cordova.file-transfer" version="0.4.6" />
    <gap:plugin name="org.apache.cordova.file" version="1.3.1" />

    <gap:plugin name="org.apache.cordova.network-information" version="0.2.12" />
    <gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />
    <gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
    <gap:plugin name="org.apache.cordova.media" version="0.2.13" />
    <gap:plugin name="org.apache.cordova.device" version="0.2.12" />
    <gap:plugin name="org.apache.cordova.camera" version="0.3.2" />
    <gap:plugin name="org.apache.cordova.media-capture" version="0.3.3" />
    <gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />

    <!--
    <gap:plugin name="org.apache.cordova.contacts" version="0.2.13" />
    <gap:plugin name="org.apache.cordova.device-orientation" version="0.3.9" />
    <gap:plugin name="org.apache.cordova.vibration" version="0.3.11" />-->
    <!--=======================================================================================-->
</widget>